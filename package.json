{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:prep && npm run build:node && npm run build:vite",
    "build:prep": "node ./utils --rm",
    "build:node": "npx tsc",
    "build:vite": "npm run build --prefix ./vite",
    "watch": "concurrently \" npm run watch:vite \" \" npm run watch:node \"",
    "watch:vite": "npm run watch --prefix ./vite",
    "watch:node": "concurrently \"npx tsc --watch --preserveWatchOutput\" \"nodemon -q dist/node/index.js\"",
    "check:vite": "npm run check --prefix ./vite",
    "start": "node dist/node/index.js",
    "postinstall": "cd vite && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
  }
}
